#121
x = float(input("x="))
n = int(input("n="))
p = 0
for i in range(1, n+1):
    p += (x**(4*i-3)) / (4*i-3)
print("P =", p)


#122
from math import cos
# Ներմուծում
x = float(input("x="))
n = int(input("n="))
# Սկզբնական արժեք
p = 1
# Արտահայտության հաշվարկ
for i in range(1, n+1):
    p *= cos((2*i-1)*x) / (2**(2*i-1))
# Արդյունք
print("P =", p)


#126
