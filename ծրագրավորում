# 264
list=[2,3,5,4,8,8,7,4,6,8]
max=list[0]
min=list[0]
for item in list:
    if item<min:
        min=item
    if item>max:
        max=item
print(max+min)


# 265
from math import*
list=[2,3,5,4,8,8,7,4,6,8]
min=list[0]
ind=0
for i in range(len(list)):
    if list[i]<min:
        min=list[i]
        ind=i
print(ind+min)


