#149
x=int(input("x="))
e=2.71
for k in range(1,15):
    k+=1
    if x>2 and x<5:
        y=7*e**(k+3)
    elif x>0 and x<=2:
        y=8*x*k**5
    else:
        y=7
print(y)


#150
x=int(input("x= "))
for k in range(2,11):
    k+=1 
    if x>3 and x<5:
        y=x**(k+6)
    elif x<=3:
        y=x+k**4 
    else:
        y=5**6 
print(y)


#221
list=[1,-2,3,-4,5,-6,7,-8,9]
s=0 #գումար
q=0 #քանակ
for item in list:
    if item>0:
        s+=item
        q+=1
print(s/q)


